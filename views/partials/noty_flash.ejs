
<%if(flash.success && flash.success.length>0){%> 
<script>
        var notyf = new Notyf();
notyf
  .success({
    message: '<%=flash.success%>',
    dismissible: true,
    duration :5000,
    position :{
        x: "right",
        y:"top"
    }
  })
  
</script>
  <%}%>

<%if(flash.error && flash.error.length>0){%> 
<script>
        var notyf = new Notyf();
notyf
  .error({
    message: '<%=flash.error%>',
    dismissible: true,
    duration :5000,
    position :{
        x: "right",
        y:"top"
    }
  })
  
</script>
  <%}%>
      

